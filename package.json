{
  "name": "ops.receiver",
  "version": "1.1.0",
  "description": "Teamcity Github integration for Dialogica",
  "engine": "node 0.12.7",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha",
    "start": "node index.js",
    "tunnel": "./utilities/ngrok http ${OPS_PORT:-8000}",
    "build": "docker build -t dialogica/ops.receiver .",
    "test:post": "curl -vX POST http://localhost:${OPS_PORT:-8000}/new-repo -d @test/stub/webhook.json --header \"Content-Type: application/json\"",
    "docker": "docker run -p ${OPS_PORT:-8000}:8000 -d dialogica/ops.receiver"
  },
  "author": "Garbrand van der Molen <garbrand@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "chai": "^3.4.0",
    "mocha": "^2.3.3",
    "mockery": "^1.4.0"
  },
  "dependencies": {
    "bluebird": "^3.0.0",
    "body-parser": "^1.14.1",
    "express": "^4.13.3",
    "request": "^2.65.0"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/informaat-ipe/receiver"
  },
  "keywords": [
    "ops"
  ],
  "bugs": {
    "url": "https://github.com/informaat-ipe/receiver/issues"
  }
}
